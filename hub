-- Create the GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ScriptHub"
ScreenGui.Parent = game.CoreGui

-- Create the MainFrame
local MainFrame = Instance.new("Frame")
MainFrame.Name = "MainFrame"
MainFrame.Size = UDim2.new(0, 250, 0, 250) -- Increased size
MainFrame.Position = UDim2.new(0.5, -125, 0.5, -125) -- Centered
MainFrame.BackgroundTransparency = 0.5
MainFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
MainFrame.BorderSizePixel = 0
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

-- Title
local Title = Instance.new("TextLabel")
Title.Name = "Title"
Title.Size = UDim2.new(1, 0, 0, 30)
Title.Position = UDim2.new(0, 0, 0, 0)
Title.BackgroundTransparency = 0.5
Title.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
Title.BorderSizePixel = 0
Title.Font = Enum.Font.SourceSans
Title.Text = "Orbit FE Hub"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextSize = 18
Title.Parent = MainFrame

-- Create the Fly button
local FlyButton = Instance.new("TextButton")
FlyButton.Name = "FlyButton"
FlyButton.Size = UDim2.new(0, 120, 0, 40) -- Adjusted size
FlyButton.Position = UDim2.new(0.25, 0, 0.2, 0) -- Positioned
FlyButton.AnchorPoint = Vector2.new(0.5, 0) -- Anchored to the top
FlyButton.BackgroundTransparency = 0.5
FlyButton.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
FlyButton.BorderSizePixel = 0
FlyButton.Font = Enum.Font.SourceSans
FlyButton.Text = "Fly"
FlyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
FlyButton.TextSize = 18
FlyButton.Parent = MainFrame
FlyButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/brandonmakerlol/fly-script/main/fly%20script%202"))() -- Replace with your actual fly script URL
end)

-- Create the Fling All button
local FlingAllButton = Instance.new("TextButton")
FlingAllButton.Name = "FlingAllButton"
FlingAllButton.Size = UDim2.new(0, 120, 0, 40) -- Adjusted size
FlingAllButton.Position = UDim2.new(0.75, 0, 0.2, 0) -- Positioned
FlingAllButton.AnchorPoint = Vector2.new(0.5, 0) -- Anchored to the top
