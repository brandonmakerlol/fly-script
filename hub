FlingAllButton.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
FlingAllButton.BorderSizePixel = 0
FlingAllButton.Font = Enum.Font.SourceSans
FlingAllButton.Text = "Fling All"
FlingAllButton.TextColor3 = Color3.fromRGB(255, 255, 255)
FlingAllButton.TextSize = 18
FlingAllButton.Parent = MainFrame
FlingAllButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://pastebin.com/raw/zqyDSUWX"))() -- Replace with your actual fling all script URL
end)

-- Create the Infinite Jump button
local InfiniteJumpButton = Instance.new("TextButton")
